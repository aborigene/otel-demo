{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "todo-tree.general.tags": ["INSTRUMENT", "FYI"],
  "todo-tree.highlights.customHighlight": {
    "INSTRUMENT": {
      "foreground": "yellow",
      "background": "grey",
      "iconColour": "yellow",
      "icon": "telescope",
      "gutterIcon": true,
      "type": "line"
    },
    "FYI": {
      "foreground": "blue",
      "background": "white",
      "icon": "info"
    }
  }
}
